            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.383"/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <TextBox x:Name="t2" HorizontalAlignment="Left" Height="35" Margin="339,69,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="116" RenderTransformOrigin="0.5,0.5">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.383"/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <Button x:Name="b2" Content="Button" HorizontalAlignment="Left" Margin="344,144,0,0" VerticalAlignment="Top" Width="109" Height="63" Click="Button2_Click"/>
        <TextBox x:Name="t3" HorizontalAlignment="Left" Height="35" Margin="203,69,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="116" RenderTransformOrigin="0.5,0.5">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
