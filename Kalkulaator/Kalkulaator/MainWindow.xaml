                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <TextBox x:Name="t4" HorizontalAlignment="Left" Height="35" Margin="336,249,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="116" RenderTransformOrigin="0.5,0.5">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.383"/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <RadioButton x:Name="r1" Content="RadioButton" HorizontalAlignment="Left" Height="18" Margin="184,162,0,0" VerticalAlignment="Top" Width="98"/>

    </Grid>
</Window>
